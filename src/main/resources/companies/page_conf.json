{
	"name" : "Dfm",
	"website": "https://www.dfm.ae/issuers/listed-securities/financial-reports",
	"standardDate" : "MM-dd-yyyy",
	"pagination": [
		{
			"name" : "Click Last to Enable First",
			"clicks": "1",
			"selectorType" : "xpath",
			"selector" : "//*[@id=\"btnLast\"]",
			"clickOnLoad" : "true",
			"index" : 0,
			"incrementation" : 1
		}, {
			"name" : "Click First to Enable Next",
			"clicks": "1",
			"selectorType" : "xpath",
			"selector" : "//*[@id=\"btnFirst\"]",
			"clickOnLoad" : "true",
			"index" : 0,
			"incrementation" : 1
		}, {
			"name" : "Click Next",
			"clicks": "3",
			"selectorType" : "xpath",
			"selector" : "//*[@id=\"btnNext\"]",
			"clickOnLoad" : "false",
			"index" : 0,
			"incrementation" : 1
		}
	],
	"crawlingSections" : [
		{
			"dateDetails" : {
				"datePattern" : "pattern-2",
				"fullDate" : { 
					"name" : "Date",
					"type" : "txtOut",
					"selector" : "ancestor::div/span[@class='my-date']",
					"selectorType" : "xpath",
					"splitter" : " ",
					"position" : 0
				}
			},
			"basicDetails" : [
				{
					"name": "Symbol",
					"type" : "txtOut",
					"selectorType": "cssSelector",
					"selector": "#container > div.feed-table-container > div > table > tbody > tr:nth-child(n) > td.it1 > div > span.name > a"
				}, {
					"name": "Event",
					"type" : "txtOut",
					"selectorType": "xpath",
					"selector": "ancestor::td/p/a"
				}, {
					"name": "Url",
					"type" : "attributeOut",
					"ref" : "href",
					"selectorType": "xpath",
					"selector": "ancestor::span/a"
				}
			]
		}
	]
}